<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="msapplication-tap-highlight" content="no" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
    <title>All Lists</title>
    <link rel="stylesheet" type="text/css" href="content/css/AllLists.css">
</head>
<body>
	<div id = "all">
		<ul>
		<li><a href="/random"/>Random!</a></li>
		@Each
		<li><a href="/rank?list=@Current">@Current</a>  (<a href="/ranking/@Current">results</a>)</li>
		@EndEach
		</ul>
	</div>	
</body>
</html>
