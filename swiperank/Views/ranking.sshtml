<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="msapplication-tap-highlight" content="no" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
    <title>Ranking</title>
    <link rel="stylesheet" type="text/css" href="content/css/style.css">
</head>
<body>
	<div id = "rerank"><a href="/rank?list=@Model.ListName">Re-rank this list</a></div>
	<div id = "all">
		<ol>
		@Each
		<li>@Current.name<br/><img src="@Current.img" /></li>
		@EndEach
		</ol>
	</div>	
</body>
</html>
